}
.responsive-layout-fluid #search-block-form .form-submit {
  background-position: 0 2px;
  background-size: 14px auto;
  border: 0 solid #FFFFFF !important;
  float: left;
  margin-top: 2px;
}
.responsive-layout-fluid .block.normal{
	display:none;
}
.responsive-layout-fluid.not-front #zone-content {
