}
.responsive-layout-fluid #block-nice-menus-1 li.menuparent .menuparent:hover > a {
  background: url("/sites/all/themes/subomega/images/menu_arrow_down.png") no-repeat scroll 95% center ;
}
.responsive-layout-fluid #block-nice-menus-1 li.menuparent a {
