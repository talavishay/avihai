/*block- 4*/ 
#block-block-4 li:first-child {
  background-color: #e4703a;
}
#block-block-4 li:nth-child(2),#block-block-4 li:nth-child(3) {
  background-color: #873661;
}
#block-block-4 li:nth-child(4) {
  background-color: #3E5B79;
}
#block-block-4 li:nth-last-child(1n+5)  {
border-radius: 5px;
font-size: 22px;
margin: 0.5% 0 !important;
padding:3% 2%  !important;
position: static;
width: 96%;
}
#block-block-4 li  {
list-style: none;
}
#block-block-4 li a {
  color: #FFFFFF;
}
#block-block-4 li:nth-child(n+5) a {
background-color: #00748F;
border-radius: 5px;
display: block;
margin: 0 1%;
padding: 10% 0;
text-align: center;

}
#block-block-4 li:nth-child(n+5) {
display: block;
float: right;
font-size: 16px;
letter-spacing: -0.6px;
margin: 0 0.4%;
padding: 0;
position: relative;
white-space: nowrap;
width: 24.2%;
word-break: break-all;
word-wrap: normal;
font-weight: bold;

}
#block-block-4 li:nth-child(5) {
padding:  0px !important;
margin: 0px !important;
}
#block-block-4 li:nth-child(8) {
  margin-left: 0 !important;
}

