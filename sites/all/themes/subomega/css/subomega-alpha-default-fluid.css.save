}



.i18n-en .menu .en.active,
.i18n-he .menu .he.active{
	display: none !important;
}
.responsive-layout-fluid .zone{
	padding : 0 5px;
}
.responsive-layout-fluid #zone-content {
  padding:  0 3%;
}
.responsive-layout-fluid.front #zone-content {
 padding: 0 2%;
}
.responsive-layout-fluid .mobile {
     border-radius: 5px;
     box-shadow: 0px 0px 31px -17px #FF0000 inset, 0px 0px 0px 1px rgba(0, 0, 0, 0.5);
     display: block;
}



.logo-img {
     display: inline-block;
     float: right;
